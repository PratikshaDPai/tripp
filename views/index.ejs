<!-- views/index.ejs -->

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Plants App</title>
    <link rel="stylesheet" href="/css/styles.css" />
  </head>
  <body class="vertical">
    <div class="container">
      <img src="/assets/placeholder.png" class="logo" alt="trip logo" />
      <h1>Welcome to Tripp!</h1>
      <p class="subtitle">Log your journeys across the world!</p>
      <form action="/trips" method="GET">
        <button type="submit" class="get-started-button">Get Started!</button>
      </form>
    </div>
    <iframe
      width="600"
      height="450"
      style="border: 0"
      loading="lazy"
      allowfullscreen
      referrerpolicy="no-referrer-when-downgrade"
      src="https://www.google.com/maps/embed/v1/place?key=AIzaSyDYtAa3VLCIa-H4HNonfGuqyS9nj7N_Ac8
    &q=Florence"
    >
    </iframe>
  </body>
</html>
